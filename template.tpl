const setInWindow = require('setInWindow');
const injectScript = require('injectScript');
const encodeUriComponent = require('encodeUriComponent');
const callInWindow = require('callInWindow');
const copyFromWindow = require('copyFromWindow');
const logToConsole = require('logToConsole');
const getUrl = require('getUrl');

const action = () => {
  if (data.actionDropdown === 'Event') {
    var eventProperties;
    if (data.eventProperties) {
      eventProperties = data.eventProperties.reduce(function(result, item) {
	    result[item.propertyName] = item.propertyValue;
	    return result;
      }, {});
    } 
    callInWindow('Sprig._queue.push', ['track', data.eventName, eventProperties]);
    
    if (data.debugMode) {
      logToConsole('Sprig: Event tracked: ', data.eventName);
      logToConsole('Sprig: Event Properties: ', eventProperties);
    }
  } else if (data.actionDropdown === 'Attribute') {
    callInWindow('Sprig._queue.push', ['setAttribute', data.attributeKey, data.attributeValue]);
    
    if (data.debugMode) {
      logToConsole('Sprig: Attribute sent: ', data.attributeKey, data.attributeValue);
    }
  } else if (data.actionDropdown === 'User ID') {
    callInWindow('Sprig._queue.push', ['setUserId', data.userId]);
    
    if (data.debugMode) {
      logToConsole('Sprig: User ID set: ', data.userId);
    }
  } else if (data.actionDropdown === 'Logout User') {
    callInWindow('Sprig._queue.push', ['logoutUser']);
    
    if (data.debugMode) {
      logToConsole('Sprig: Logout User');
    }
  } else if (data.actionDropdown === 'Install') {
    // no-op
    if (data.debugMode) {
      logToConsole('Sprig: Install', getUrl('path'));
    }
  } else if (data.actionDropdown === 'Email') {
    callInWindow('Sprig._queue.push', ['setEmail', data.email]);
    
    if (data.debugMode) {
      logToConsole('Sprig: Email set: ', data.email);
    }
  } else if (data.actionDropdown == 'Dismiss Active Survey') {
    callInWindow('Sprig._queue.push', ['dismissActiveSurvey']);
    
    if (data.debugMode) {
      logToConsole('Sprig: Dismissed Active Survey');
    }
  }
  data.gtmOnSuccess();
};

const onSetupSuccess = () => {
  logToConsole('Sprig: Script setup success.');
  action();
};

const onSetupFailure = () => {
  logToConsole('Sprig: Script setup failed.');
  data.gtmOnFailure();
};

const setup = (onSuccess, onFailure) => {
  setInWindow('Sprig', function(){
    callInWindow('Sprig._queue.push', arguments);
  });
  setInWindow('Sprig.appId', data.envId);
  setInWindow('Sprig.debugMode', data.debugMode);
  setInWindow('Sprig._gtm', 1);
  setInWindow('Sprig._queue', []);
  injectScript('https://cdn.sprig.com/shim.js?id='+encodeUriComponent(data.envId), onSetupSuccess, onSetupFailure);
};

const Sprig = copyFromWindow('Sprig');
if (Sprig && Sprig.appId) {
  action();
} else if (!data.skipInstallation) {
  setup();
} else {
  logToConsole('Sprig: Invoked script before separate installation completed.');
  data.gtmOnFailure();
}


